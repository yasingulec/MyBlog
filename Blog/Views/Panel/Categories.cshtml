@model IEnumerable<Category>
    <div class="container">
        <div class="menucontrol" style="margin-top:20%">
            <table class="table table-hover table-bordered">
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td>@item.Name</td>
                            <td>
                                <a asp-action="Edit" asp-controller="Panel" asp-route-id="@item.Id">
                                    <i class="fas fa-edit"></i>
                                </a>
                            </td>
                            <td>
                                <a asp-action="RemoveCategory" asp-controller="Panel" asp-route-id="@item.Id">
                                    <i class="fas fa-trash-alt "> </i>

                                </a>
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>